<script>
    function getsubcategoryId() {
        let catId = document.getElementById('categoryId').value;
        $.ajax({
            url: '/extracategory/ajaxGetSubCategory',
            method: 'post',
            data: {
                'categoryId': catId
            },
            success: function (res) {
                $('#subcategoryId').html(res);
            }
        })
    }
    function getExtraCategoryData() {
        let subId = document.getElementById('subcategoryId').value;
        $.ajax({
            url: '/brand/ajaxGetExtraCategory',
            method: 'post',
            data: {
                'subcategoryId': subId
            },
            success: function (res) {
                $('#extracategoryId').html(res);
            }
        })
    }
    function getBrandTypeData() {
        let catId = document.getElementById('categoryId').value;
        let subId = document.getElementById('subcategoryId').value;
        let extraId = document.getElementById('extracategoryId').value;
        $.ajax({
            url: '/product/ajaxGetBrandType',
            method: 'post',
            data: {
                'categoryId': catId,
                'subcategoryId': subId,
                'extracategoryId': extraId
            },
            success:function(res){
                $('#BrandTypeData').html(res);
            }
        })
    }
</script>
